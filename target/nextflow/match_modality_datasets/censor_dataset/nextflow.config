  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    censor_dataset__input_mod1 = "viash_no_value"
    censor_dataset__input_mod2 = "viash_no_value"
    censor_dataset__output_mod1 = "viash_no_value"
    censor_dataset__output_mod2 = "viash_no_value"
    censor_dataset__output_solution = "viash_no_value"
    id = ""
    testScript = "test.py"
    testResources = [ "test.py", "../../../../resources_test" ]
    censor_dataset {
      name = "censor_dataset"
      container = "match_modality_datasets_censor_dataset"
      containerTag = "0.4.0"
      containerRegistry = "openproblems"
      command = "censor_dataset"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py", "../../../../resources_test" ]
      }
      arguments {
        input_mod1 {
          name = "input_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.censor_dataset__input_mod1}"
          example = "dataset.h5ad"
          description = "The first modality of the input dataset to be censored"
        }
        input_mod2 {
          name = "input_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.censor_dataset__input_mod2}"
          example = "dataset.h5ad"
          description = "The second modality of the input dataset to be censored"
        }
        output_mod1 {
          name = "output_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.censor_dataset__output_mod1}"
          example = "dataset_censored.h5ad"
          description = "A censored version of the first modality of the dataset."
        }
        output_mod2 {
          name = "output_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.censor_dataset__output_mod2}"
          example = "dataset_censored.h5ad"
          description = "A censored version of the second modality of the dataset."
        }
        output_solution {
          name = "output_solution"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.censor_dataset__output_solution}"
          example = "dataset_solution.h5ad"
          description = "A solution of the dataset"
        }
      }
    }
  }