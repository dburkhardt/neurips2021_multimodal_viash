  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    calculate_cor__input_prediction = "viash_no_value"
    calculate_cor__input_solution = "viash_no_value"
    calculate_cor__output = "viash_no_value"
    id = ""
    testScript = "test.R"
    testResources = [ "test.R", "../../../../resources_test" ]
    calculate_cor {
      name = "calculate_cor"
      container = "predict_modality_metrics_calculate_cor"
      containerTag = "main_build"
      containerRegistry = "openproblems"
      command = "calculate_cor"
      tests {
        isDefined = true
        testScript = "test.R"
        testResources = [ "test.R", "../../../../resources_test" ]
      }
      arguments {
        input_prediction {
          name = "input_prediction"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = true
          multiple_sep = ":"
          value = "${params.calculate_cor__input_prediction}"
          example = "prediction.h5ad"
          description = "Prediction by a method."
        }
        input_solution {
          name = "input_solution"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = true
          multiple_sep = ":"
          value = "${params.calculate_cor__input_solution}"
          example = "solution.h5ad"
          description = "Gold standard solution."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.calculate_cor__output}"
          example = "output.h5ad"
          description = "Metric scores by comparing the prediction to the gold standard solution."
        }
      }
    }
  }