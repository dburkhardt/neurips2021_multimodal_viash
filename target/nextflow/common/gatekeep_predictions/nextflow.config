  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    gatekeep_predictions__input_prediction = "viash_no_value"
    gatekeep_predictions__input_solution = "viash_no_value"
    gatekeep_predictions__output = "viash_no_value"
    id = ""
    testScript = "test.R"
    testResources = [ "test.R", "../../../../resources_test" ]
    gatekeep_predictions {
      name = "gatekeep_predictions"
      container = "common_gatekeep_predictions"
      containerTag = "main_build"
      containerRegistry = "openproblems"
      command = "gatekeep_predictions"
      tests {
        isDefined = true
        testScript = "test.R"
        testResources = [ "test.R", "../../../../resources_test" ]
      }
      arguments {
        input_prediction {
          name = "input_prediction"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = true
          multiple_sep = ":"
          value = "${params.gatekeep_predictions__input_prediction}"
          example = "prediction.h5ad"
          description = "Prediction by a method."
        }
        input_solution {
          name = "input_solution"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = true
          multiple_sep = ":"
          value = "${params.gatekeep_predictions__input_solution}"
          example = "solution.h5ad"
          description = "Gold standard solution."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.gatekeep_predictions__output}"
          example = "output.tsv"
          description = "TSV file of the solution + prediction pairing."
        }
      }
    }
  }