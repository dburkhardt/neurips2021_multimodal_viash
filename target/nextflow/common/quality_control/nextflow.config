  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    quality_control__input_rna = "viash_no_value"
    quality_control__input_mod2 = "viash_no_value"
    quality_control__output_rna = "viash_no_value"
    quality_control__output_mod2 = "viash_no_value"
    quality_control__min_counts_per_gene = "100"
    quality_control__min_counts_per_cell = "100"
    id = ""
    testScript = "test.R"
    testResources = [ "test.R", "../../../../resources_test" ]
    quality_control {
      name = "quality_control"
      container = "common_quality_control"
      containerTag = "0.2.0"
      containerRegistry = "openproblems"
      command = "quality_control"
      tests {
        isDefined = true
        testScript = "test.R"
        testResources = [ "test.R", "../../../../resources_test" ]
      }
      arguments {
        input_rna {
          name = "input_rna"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.quality_control__input_rna}"
          example = "input.h5ad"
          description = "Raw dataset in a h5ad file."
        }
        input_mod2 {
          name = "input_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.quality_control__input_mod2}"
          example = "input.h5ad"
          description = "Raw dataset in a h5ad file."
        }
        output_rna {
          name = "output_rna"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.quality_control__output_rna}"
          example = "output.h5ad"
          description = "Normalized h5ad file."
        }
        output_mod2 {
          name = "output_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.quality_control__output_mod2}"
          example = "output.h5ad"
          description = "Normalized h5ad file."
        }
        min_counts_per_gene {
          name = "min_counts_per_gene"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.quality_control__min_counts_per_gene}"
          dflt = "100"
          description = "Minimum counts per gene."
        }
        min_counts_per_cell {
          name = "min_counts_per_cell"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.quality_control__min_counts_per_cell}"
          dflt = "100"
          description = "Minimum counts per cell."
        }
      }
    }
  }