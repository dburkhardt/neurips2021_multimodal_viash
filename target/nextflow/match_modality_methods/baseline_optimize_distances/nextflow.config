  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    baseline_optimize_distances__input_mod1 = "viash_no_value"
    baseline_optimize_distances__input_mod2 = "viash_no_value"
    baseline_optimize_distances__output = "viash_no_value"
    baseline_optimize_distances__distance_method = "spearman"
    baseline_optimize_distances__n_dims = "10"
    baseline_optimize_distances__n_ga_pop = "50"
    baseline_optimize_distances__n_ga_iter = "100"
    id = ""
    testScript = "test.R"
    testResources = [ "test.R", "../../../../resources_test" ]
    baseline_optimize_distances {
      name = "baseline_optimize_distances"
      container = "match_modality_methods_baseline_optimize_distances"
      containerTag = "main_build"
      containerRegistry = "openproblems"
      command = "baseline_optimize_distances"
      tests {
        isDefined = true
        testScript = "test.R"
        testResources = [ "test.R", "../../../../resources_test" ]
      }
      arguments {
        input_mod1 {
          name = "input_mod1"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_optimize_distances__input_mod1}"
          example = "dataset_mod1.h5ad"
          description = "Censored dataset."
        }
        input_mod2 {
          name = "input_mod2"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_optimize_distances__input_mod2}"
          example = "dataset_mod2.h5ad"
          description = "Censored dataset."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_optimize_distances__output}"
          example = "output.h5ad"
          description = "Dataset with predicted values for modality2."
        }
        distance_method {
          name = "distance_method"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_optimize_distances__distance_method}"
          dflt = "spearman"
          description = "The distance method to use. Possible values are euclidean, pearson, spearman and others."
        }
        n_dims {
          name = "n_dims"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_optimize_distances__n_dims}"
          dflt = "10"
          description = "Number of dimensions to use."
        }
        n_ga_pop {
          name = "n_ga_pop"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_optimize_distances__n_ga_pop}"
          dflt = "50"
          description = "Population size of genetic algorithm."
        }
        n_ga_iter {
          name = "n_ga_iter"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.baseline_optimize_distances__n_ga_iter}"
          dflt = "100"
          description = "Number of GA iterations to perform."
        }
      }
    }
  }